    <section ident="section_1_test_4-2_comprehension">
      <item ident="q_0" title="Noms de domaine">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.essay.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  Comment s'appelle l'organisme qui gère les noms de domaine et les adresses IP ? &lt;/strong&gt; Précisez votre réponse pour la France et au niveau mondial.&lt;/p&gt;</mattext>
          </material>
          <response_str ident="response_86c822a1-1e5e-4ecd-abb2-a1664a245a3c" rcardinality="Single">
            <render_fib prompt="Box" rows="5" fibtype="String" />
          </response_str>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; ICANN pour le niveau mondial et l'AFNIC pour la France.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="q_1" title="Qu'est-ce que l'ISO ?">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  L'ISO est l'organisation internationale de normalisation. &lt;/strong&gt;&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_6416613a-dd58-4112-b4ce-1885144f5411" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_6416613a-dd58-4112-b4ce-1885144f5411">
                <material>
                  <mattext texttype="text/html">Il faut revoir la vidéo...</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; Vrai, l'ISO est bien l'organisation internationale de normalisation (International Organization for Standardization).&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="q_2" title="Noms pleinement qualifiés">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; Comment s'écrit le nom d'une machine C, située dans un domaine B, lui même dans un domaine général A ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_7739e912-020d-41d4-a692-dfdc6bfefac8" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_0">
                <material>
                  <mattext texttype="text/html">A.B.C</mattext>
                </material>
              </response_label>
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_1">
                <material>
                  <mattext texttype="text/html">A.C.B</mattext>
                </material>
              </response_label>
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_2">
                <material>
                  <mattext texttype="text/html">C.B.A</mattext>
                </material>
              </response_label>
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_3">
                <material>
                  <mattext texttype="text/html">C.A.B</mattext>
                </material>
              </response_label>
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_4">
                <material>
                  <mattext texttype="text/html">B.A.C</mattext>
                </material>
              </response_label>
              <response_label ident="answer_7739e912-020d-41d4-a692-dfdc6bfefac8_5">
                <material>
                  <mattext texttype="text/html">B.C.A</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; C.B.A \: Le nom de la machine, puis le nom du domaine de 1er niveau et enfin le nom du domaine général.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
