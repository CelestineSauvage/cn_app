          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  Qu'est-ce qui joue sur la vitesse d'une connexion ? &lt;/strong&gt;&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_fbde9b7e-0818-4a0d-b03f-da9333e54722" rcardinality="Multiple">
            <render_choice shuffle="No">
              <response_label ident="answer_fbde9b7e-0818-4a0d-b03f-da9333e54722_0">
                <material>
                  <mattext texttype="text/html">Le débit maximal admissible du lien qui me raccorde au réseau local.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_fbde9b7e-0818-4a0d-b03f-da9333e54722_1">
                <material>
                  <mattext texttype="text/html">Le nombre d'utilisateurs de ce lien.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_fbde9b7e-0818-4a0d-b03f-da9333e54722_2">
                <material>
                  <mattext texttype="text/html">Le nombre de messages qui passent par les mêmes routeurs que les miens.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_fbde9b7e-0818-4a0d-b03f-da9333e54722_3">
                <material>
                  <mattext texttype="text/html">Le nombre de requêtes arrivant sur la machine avec qui je désire échanger des messages.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_fbde9b7e-0818-4a0d-b03f-da9333e54722_4">
                <material>
                  <mattext texttype="text/html">L'âge de l'utilisateur.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
