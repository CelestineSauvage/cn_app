    <section ident="section_1_test_3-2_comprehension">
      <item ident="q_0" title="les supports de connexion">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_response.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  Quels supports sont utilisés pour la transmission de données &lt;/strong&gt; et qui peuvent donc servir pour une connexion internet ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_e0c5f0de-7d93-4480-9de2-21194865e152" rcardinality="Multiple">
            <render_choice shuffle="No">
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_0">
                <material>
                  <mattext texttype="text/html">L'eau dans un tuyau.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_1">
                <material>
                  <mattext texttype="text/html">L'électricité dans les câbles téléphonique.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_2">
                <material>
                  <mattext texttype="text/html">L'électricité dans les câbles spécialisés.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_3">
                <material>
                  <mattext texttype="text/html">La lumière dans les fibres optiques.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_4">
                <material>
                  <mattext texttype="text/html">Les ondes électromagnétiques dans l'air.</mattext>
                </material>
              </response_label>
              <response_label ident="answer_e0c5f0de-7d93-4480-9de2-21194865e152_5">
                <material>
                  <mattext texttype="text/html">L'électricité dans les câbles des prises électriques.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; Tout, sauf l'eau, évidemment !&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_0">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Pas à notre connaissance, mais pourquoi pas ?&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_1">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, l'ADSL&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_2">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, l'électricité dans les câbles des prises électriques (Voir http://fr.wikipedia.org/wiki/Courants_porteurs_en_ligne)&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_3">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, aussi.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_4">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, Wifi, bluetooth, réseau téléphonique 2G, 3G, 4G, etc.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_5">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, voir http://fr.wikipedia.org/wiki/Courants_porteurs_en_ligne&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="q_1" title="vitesse de connexion">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_response.v0p1</fieldentry>
