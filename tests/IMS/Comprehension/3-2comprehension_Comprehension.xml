        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  Quels supports sont utilisés pour la transmission de données &lt;/strong&gt; et qui peuvent donc servir pour une connexion internet ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_ff25ec6f-c5f4-4bad-99da-387633c8bdec" rcardinality="Multiple">
            <render_choice shuffle="No">
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_0">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'eau dans un tuyau.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_1">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'électricité dans les câbles téléphonique.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_2">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'électricité dans les câbles spécialisés.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_3">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;La lumière dans les fibres optiques.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_4">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;Les ondes électromagnétiques dans l'air.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_ff25ec6f-c5f4-4bad-99da-387633c8bdec_5">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'électricité dans les câbles des prises électriques.&lt;/p&gt;</mattext>
