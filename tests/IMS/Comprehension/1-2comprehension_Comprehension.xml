    <section ident="section_1_test_1-2_comprehension">
      <item ident="q_0" title="Pourquoi représenter avec des nombres ?">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; Pourquoi faut-il représenter les textes, images, son,etc, par des nombres dans un ordinateur ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_919fbaac-5793-495c-a22b-911369f28089" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_919fbaac-5793-495c-a22b-911369f28089_0">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;C'est un choix industriel.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_919fbaac-5793-495c-a22b-911369f28089_1">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;Les ordinateurs ont été inventés par des mathématiciens.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_919fbaac-5793-495c-a22b-911369f28089_2">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;Tout ordinateur est fondamentalement une machine qui calcule avec desnombres.&lt;/p&gt;</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; Un ordinateur ne manipule que des nombres, tout doit donc être représenté sous forme de nombres pour qu'il puisse le manipuler.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_0">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Non, les industriels n'avaient pas le choix.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_1">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Non, les mathématiciens savent manipuler autre chose que des nombres, et les ordinateurs sont le fruit de l'interaction entre de nombreuses sciences.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_2">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui, comme un ordinateur ne manipule que des nombres,tout doit être représenté sous forme de nombres être manipulé par un ordinateur.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="q_1" title="Numérisation">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; Que signifie &lt;em&gt;  numérisation &lt;/em&gt; ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_76c8372b-1a8c-4452-8105-e35561d2dd0c" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_76c8372b-1a8c-4452-8105-e35561d2dd0c_0">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'opération qui consiste à représenter sous forme de nombres uneinformation quelle qu'elle soit.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_76c8372b-1a8c-4452-8105-e35561d2dd0c_1">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;L'opération qui consiste à citer tous les nombres (1, 2, 3, 4, ….).&lt;/p&gt;</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; La numérisation est l'opération qui consiste à représenter sous forme de nombres une information, quelle qu'elle soit.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_0">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Oui !&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_1">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Non, citer tous les nombres c'est compter...&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
