    <section ident="section_1_test_1-2_comprehension">
      <item ident="q_0" title="Pourquoi représenter avec des nombres ?">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; Pourquoi faut-il représenter les textes, images, son,etc, par des nombres dans un ordinateur ?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd_0">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;C'est un choix industriel.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd_1">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;Les ordinateurs ont été inventés par des mathématiciens.&lt;/p&gt;</mattext>
                </material>
              </response_label>
              <response_label ident="answer_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd_2">
                <material>
                  <mattext texttype="text/html">&lt;p&gt;Tout ordinateur est fondamentalement une machine qui calcule avec desnombres.&lt;/p&gt;</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
          <respcondition title="">
            <conditionvar>
              <varequal respident="response_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd">answer_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd_0</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="feedb_0" />
          </respcondition>
          <respcondition title="">
            <conditionvar>
              <varequal respident="response_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd">answer_a815b50b-e8f8-46fc-b8e3-3f74dc3a91dd_1</varequal>
