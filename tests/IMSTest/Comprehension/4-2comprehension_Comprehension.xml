      <item ident="q_0" title="Noms de domaine">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.essay.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  Comment s'appelle l'organisme qui gère les noms de domaine et les adresses IP ? &lt;/strong&gt; Précisez votre réponse pour la France et au niveau mondial.&lt;/p&gt;</mattext>
          </material>
          <response_str ident="response_6a7aef9a-e609-4799-a20b-d772f4b00e46" rcardinality="Single">
            <render_fib prompt="Box" rows="5" fibtype="String" />
          </response_str>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; ICANN pour le niveau mondial et l'AFNIC pour la France.&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="q_1" title="Qu'est-ce que l'ISO ?">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>cc_question_category</fieldlabel>
              <fieldentry>Quiz Bank Compréhension</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt; &lt;strong&gt;  L'ISO est l'organisation internationale de normalisation. &lt;/strong&gt;&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response_d33d57ca-cb5e-4665-98c8-e6d063ee43d2" rcardinality="Single">
            <render_choice shuffle="No">
              <response_label ident="answer_d33d57ca-cb5e-4665-98c8-e6d063ee43d2_0">
                <material>
                  <mattext texttype="text/html">Vrai</mattext>
                </material>
              </response_label>
              <response_label ident="answer_d33d57ca-cb5e-4665-98c8-e6d063ee43d2_1">
                <material>
                  <mattext texttype="text/html">Faux</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" maxvalue="100" minvalue="0" />
          </outcomes>
          <respcondition continue="Yes" title="General feedback">
            <conditionvar>
              <other />
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="general_fb" />
          </respcondition>
          <respcondition title="Correct">
            <conditionvar>
              <varequal respident="response_d33d57ca-cb5e-4665-98c8-e6d063ee43d2">answer_d33d57ca-cb5e-4665-98c8-e6d063ee43d2_0</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="feedb_0" />
          </respcondition>
          <respcondition title="">
            <conditionvar>
              <varequal respident="response_d33d57ca-cb5e-4665-98c8-e6d063ee43d2">answer_d33d57ca-cb5e-4665-98c8-e6d063ee43d2_1</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">0</setvar>
            <displayfeedback feedbacktype="Response" linkrefid="feedb_1" />
          </respcondition>
        </resprocessing>
        <itemfeedback ident="general_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt; Vrai, l'ISO est bien l'organisation internationale de normalisation (International Organization for Standardization).&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="feedb_0">
          <flow_mat>
            <material>
              <mattext texttype="text/html">Exact !</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
